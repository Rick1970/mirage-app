<div class=container>
  <h1>Location Test Page</h1>

  <ul>
    <li><strong>Length of Location List: </strong>{{model.location.data.length}}</li>
    <br>
    <li><strong>List of Locations:</strong></li>
    <ul>
      <br>
      {{#each model.location.data as |location| }}
      <li>Belongs to:{{location.attributes.account}}</li>
      <li><strong>Name:</strong> {{location.attributes.location-name}}</li>
      <li><strong>Asset#:</strong> {{location.attributes.asset}}</li>
      <li><strong>Route:</strong> {{location.attributes.route}}</li>
      <li><strong>Sales Model:</strong> {{location.attributes.sales-model}}</li>
      <li><strong>Pricing:</strong> {{location.attributes.pricing}}</li>
      <li><strong>Tax:</strong> {{location.attributes.tax}}</li>
      <li><strong>Commission:</strong>{{location.attributes.com}}</li>
      {{/each}}
    </ul>
  </ul>
  {{#link-to 'index'}}To Accounts{{/link-to}}
</div>

{{outlet}}
